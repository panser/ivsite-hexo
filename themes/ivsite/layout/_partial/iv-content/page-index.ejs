<% var paths = page.source.split("/") %>
<% paths.splice(-1,1) %>

<% var navElement = site.data.nav %>
<% paths.forEach(function (path) { %>
    <% navElement = navElement[0].items.filter(function(obj){ return obj.index === path; }) %>
<%}) %>

<a href="#" > <%= navElement[0].title %> </a>
<ul>
    <% navElement[0].items.forEach(function (subMenu){ %>
    <li>
        <a href="<%= subMenu.index %>" > <%= subMenu.title %> </a>
    </li>
    <% }) %>
</ul>
